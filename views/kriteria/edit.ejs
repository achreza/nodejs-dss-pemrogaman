<%- include ../layouts/header.ejs %> <% if (messages.error) { %>
<p style="color: red"><%- messages.error %></p>
<% } %> <% if (messages.success) { %>
<p style="color: green"><%- messages.success %></p>
<% } %>

<form action="/kriteria/edit/<%= id_kriteria %>" method="post" name="form1">
  <table width="25%" border="0">
    <tr>
      <td>Nama Kriteria</td>
      <td><input type="text" name="nm_kriteria" id="nm_kriteria" value="<%= nm_kriteria %>" /></td>
    </tr>
    <tr>
      <td>Jenis</td>
      <td><input type="text" name="jenis" value="<%= jenis %>" /></td>
    </tr>
    <tr>
      <td></td>
      <td><input type="submit" name="Submit" value="Edit" /></td>
    </tr>
  </table>
  <input type="hidden" name="_method" value="PUT" />
</form>

